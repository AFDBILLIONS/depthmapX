                        </fixedvalues>
                    </number>
                    <structure name="graphNode" id="58" repeatmin="isInGraph" repeatmax="isInGraph">
                        <structure name="bin" id="59" repeatmin="32" repeatmax="32" order="variable">
                            <description>For some odd reason synalize it skips this structure and instead displays its child elements 32 times</description>
