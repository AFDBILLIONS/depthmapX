                                    <fixedvalues>
                                        <fixedvalue name="POSDIAGONAL" value="04"/>
                                        <fixedvalue name="NEGDIAGONAL" value="08"/>
                                        <fixedvalue name="DIAGONAL" value="0C"/>
                                    </fixedvalues>
                                </binary>
                                <structure name="yesNodes" id="68" repeatmin="0">
                                    <number name="noOfNodes" mustmatch="yes" id="69" fillcolor="FFC0F7" type="integer" length="2" minval="1"/>
                                    <number name="m_distance" id="70" type="float" length="32" lengthunit="bit"/>
                                    <number name="m_occ_distance" id="71" type="float" length="32" lengthunit="bit"/>
                                    <structref name="startPixel" id="73" structure="id:72"/>
                                </structure>
                            </structure>
                            <structure name="nonDiagonal" id="76" length="0" repeatmin="0">
                                <binary name="direction" mustmatch="yes" id="77" fillcolor="FF5B1F" length="1">
                                    <fixedvalues>
                                        <fixedvalue name="NODIR" value="00"/>
                                        <fixedvalue name="HORIZONTAL" value="01"/>
                                        <fixedvalue name="VERTICAL" value="02"/>
                                        <fixedvalue name="NEGHORIZONTAL" value="10"/>
                                        <fixedvalue name="NEGVERTICAL" value="20"/>
                                    </fixedvalues>
                                </binary>
                                <structure name="yesNodes" id="78" repeatmin="0">
                                    <number name="noOfNodes" mustmatch="yes" id="79" fillcolor="FFC0F7" type="integer" length="2" minval="1"/>
                                    <number name="m_distance" id="80" type="float" length="32" lengthunit="bit"/>
                                    <number name="m_occ_distance" id="81" type="float" length="32" lengthunit="bit"/>
                                    <number name="m_length" id="82" fillcolor="F8FDC6" type="integer" length="2"/>
                                    <structref name="startPixel" id="83" structure="id:72"/>
                                    <structure name="pixelShift" id="84" length="4" repeatmin="m_length - 1" repeatmax="m_length - 1">
                                        <number name="primaryIndex" id="85" type="integer" length="2">
                                            <description>Why is this short when the noOfCellsX is int?</description>
                                        </number>
                                        <binary name="shift+runlength" id="86" disabled="yes" length="2">
                                            <description>struct ShiftLength {
       unsigned short shift : 4;
       unsigned short runlength : 12;
    };</description>
                                        </binary>
                                        <structure name="shiftrunlengthbitfields" id="87" length="2">
                                            <description>struct ShiftLength {
       unsigned short shift : 4;
       unsigned short runlength : 12;
    };</description>
                                            <number name="runlength" id="88" type="integer" length="4" lengthunit="bit"/>
                                            <number name="shift" id="89" type="integer" length="4" lengthunit="bit"/>
                                            <binary name="pad" id="90" length="8" lengthunit="bit"/>
                                        </structure>
                                    </structure>
                                </structure>
                            </structure>
                        </structure>
                        <structure name="emptybin" id="96" length="4" repeatmin="32" repeatmax="32"/>
                    </structure>
                    <structref name="vertex" id="99" structure="id:33"/>
                </structure>
            </structure>
            <structure name="DataMaps" id="102" repeatmin="0">
                <string name="map type" mustmatch="yes" id="103" type="fixed-length" length="1">
                    <fixedvalues>
